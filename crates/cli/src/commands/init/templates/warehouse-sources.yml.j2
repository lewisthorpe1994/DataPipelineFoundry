warehouse-sources:
  - name: {{ example_warehouse_source_name }}
    database:
      name: some_database
      schemas:
        - name: some_schema
          description: some schema for raw ingested data
          tables:
            - name: raw_orders
              description: Raw orders ingested from upstream system
            - name: raw_customers
              description: Raw customer data
        - name: some_other_schema
          description: some other schema for cleaned data
          tables:
            - name: cleaned_orders
              description: Cleaned and deduplicated orders
